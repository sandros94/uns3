{
  "name": "uns3",
  "version": "0.0.6",
  "description": "Runtime agnostic S3 client.",
  "repository": "sandros94/uns3",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "exports": {
    "./package.json": "./package.json",
    "./core": "./dist/core.mjs",
    "./utils": "./dist/utils.mjs",
    ".": {
      "types": "./dist/index.d.mts",
      "core": "./dist/core.mjs",
      "utils": "./dist/utils.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "obuild",
    "dev": "npx --yes listhen -w ./playground/main.ts",
    "lint": "eslint --cache . && prettier -c src test docs",
    "lint:fix": "automd && eslint --cache . --fix && prettier -c src test docs -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && pnpm prepack && changelogen --release --push",
    "test": "pnpm lint && pnpm test:types && vitest run",
    "test:coverage": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck",
    "bench": "vitest bench --run"
  },
  "resolutions": {
    "uns3": "link:."
  },
  "devDependencies": {
    "@types/node": "^22",
    "@vitest/coverage-v8": "^4.0.17",
    "automd": "^0.4.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "eslint-config-unjs": "^0.6.2",
    "obuild": "^0.4.20",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": "22"
  },
  "packageManager": "pnpm@10.28.1"
}
